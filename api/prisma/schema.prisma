// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  	provider = "prisma-client-js"
}

datasource db {
	provider = "postgresql"
	url      = env("POSTGRES_URL")
}

model Dinosaur {
	id    	Int     @id @default(autoincrement())
	species String
	name  	String
	age  	Int
}

model User {
	id    	 Int     @id @default(autoincrement())
	name  	 String
	username String  @unique
	password String
}
